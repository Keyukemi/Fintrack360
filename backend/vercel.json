{
  "version": 2,
  "builds": [
    { "src": "index.js", "use": "@vercel/node" }
  ],
  "routes": [
    { 
      "src": "/(.*)", 
      "dest": "/index.js",
      "methods": ["GET", "POST", "PUT", "PATCH", "DELETE"], 
      "headers": [
        { "key": "Access-Control-Allow-Origin", "value": "*" } 
      ]
    }
  ],
  "functions": {
    "api": {
      "maxDuration": 60
    }
  }
}
